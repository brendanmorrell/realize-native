{
  "name": "realize-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:generate": "concurrently 'npm run generate:watch' 'npx react-native run-android'",
    "android": "npx react-native run-android",
    "ios:generate": "concurrently 'npm run generate:watch' 'npx react-native run-ios'",
    "ios": "npx react-native run-ios",
    "start:generate": "concurrently 'npm run generate:watch' 'npx react-native start'",
    "start": "npx react-native start",
    "test": "jest",
    "generate": "dotenv -- graphql-codegen --config codegen.js",
    "generate:watch": "dotenv -- graphql-codegen --config codegen.js --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@emotion/native": "^11.10.0",
    "@emotion/react": "^11.10.5",
